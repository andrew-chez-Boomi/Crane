tags:
  - name: postdata
paths:
  /trigger/postdata/paylaod:
    post:
      tags:
        - postdata
      summary: "[Process] BOOMI API POST payload"
      description: "This end point is to send some paylaod jon/xml/csv to boomi process so BOOMI process will take the inout paylaod and triggered."
      parameters:
